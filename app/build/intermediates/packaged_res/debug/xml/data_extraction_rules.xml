<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive SSH credentials from cloud backup -->
        <exclude domain="sharedpref" path="ssh_credentials.xml" />
        <exclude domain="sharedpref" path="encrypted_prefs.xml" />
    </cloud-backup>
</data-extraction-rules>
